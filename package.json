{
  "name": "minimalist-es6-isomorphic-react-template",
  "version": "0.0.1",
  "description": "simple isomorphic react with hot reloading & server restarting",
  "main": "index.js",
  "dependencies": {
    "babel-loader": "^5.3.2",
    "babel-core": "^5.8.22",
    "express": "^4.13.3",
    "react-hot-loader": "^1.2.8",
    "react-router": "^0.13.3",
    "webpack-dev-server": "^1.10.1",
    "nodemon": "^1.4.1",
    "jade": "^1.11.0",
    "react": "^0.13.3",
    "babel": "^5.8.21",
    "webpack": "^1.11.0"
  },
  "devDependencies": {},
  "scripts": {
    "clean": "rm -rf lib",
    "watch-js": "babel src -d lib --experimental -w",
    "dev-server": "node lib/server/webpack",
    "server": "nodemon lib/server/server",
    "start": "npm run watch-js & npm run dev-server & npm run server",
    "build": "npm run clean && ./node_modules/.bin/babel src -d lib --experimental"
  },
  "author": "namelos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namelos/minimalist-es6-isomorphic-react-template.git"
  },
  "keywords": [
    "isomorph",
    "react",
    "hot-reload",
    "webpack",
    "es6",
    "express"
  ],
  "bugs": {
    "url": "https://github.com/namelos/minimalist-es6-isomorphic-react-template/issues"
  },
  "homepage": "https://github.com/namelos/minimalist-es6-isomorphic-react-template#readme"
}
